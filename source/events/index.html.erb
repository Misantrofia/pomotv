---
title: Events
---

<div class="events">
  <% data.events.each do |event, event_data| %>
    <h2><%= event %></h2>
    <p>
    <%= link_to event_data.url, event_data.url %>
    </p>
    <p>
      <% data.videos[event].each do |video| %>
        <%= video.speakers.join(", ") %> - <%= link_to_video video %> <br/>
      <% end %>
    </p>
  <% end %>
</div>
