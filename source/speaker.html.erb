<h1><%= name %></h1>
<% if speaker.twitter %>
<p>
  <%= link_to('@' + speaker.twitter, 'http://twitter.com/' + speaker.twitter) %><br/>
</p>
<% end %>
<% videos.each do |conference, videos| %>
  <% videos.each do |video| %>
    <h3><%= video.title %></h3>
    <p> <%= link_to conference, "/editions/#{data.editions[conference].slug}" %> </p>
    <%= partial :video_content, :locals => { :video => video } %>
  <% end %>
<% end %>
